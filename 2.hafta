# 4. İki Sayı ile İşlem Yapan Basit Hesap Makinesi
print("--- 4. Basit Hesap Makinesi ---")
s1 = float(input("İlk sayı: "))
s2 = float(input("İkinci sayı: "))
islem = input("İşlem seçin (+, -, *, /): ")
if islem == '+':
    print("Sonuç:", s1 + s2)
elif islem == '-':
    print("Sonuç:", s1 - s2)
elif islem == '*':
    print("Sonuç:", s1 * s2)
elif islem == '/':
    if s2 != 0:
        print("Sonuç:", s1 / s2)
    else:
        print("Hata: Bir sayı sıfıra bölünemez.")
else:
    print("Geçersiz işlem.")
print("-" * 20)


# 5. Hesap Makinesinde Kullanıcı Hatalarını Önleme
print("--- 5. Gelişmiş Hesap Makinesi (Hata Kontrollü) ---")
try:
    hs1 = float(input("İlk sayı: "))
    hs2 = float(input("İkinci sayı: "))
    islem_h = input("İşlem seçin (+, -, *, /): ")

    if islem_h == '+':
        print("Sonuç:", hs1 + hs2)
    elif islem_h == '-':
        print("Sonuç:", hs1 - hs2)
    elif islem_h == '*':
        print("Sonuç:", hs1 * hs2)
    elif islem_h == '/':
        print("Sonuç:", hs1 / hs2)
    else:
        print("Geçersiz işlem girdiniz.")
except ValueError:
    print("Hata: Lütfen sadece sayısal değerler giriniz.")
except ZeroDivisionError:
    print("Hata: Bir sayı sıfıra bölünemez.")
print("-" * 20)


# 6. +/- İşaretine Her Basıldığında Sayıyı Arttırma/Azaltma
print("--- 6. Sayaç Uygulaması ---")
sayac = 0
print(f"Başlangıç değeri: {sayac}. Çıkmak için 'q' yazın.")
while True:
    komut = input("Arttırmak için '+', azaltmak için '-' girin: ")
    if komut == '+':
        sayac += 1
    elif komut == '-':
        sayac -= 1
    elif komut.lower() == 'q':
        break
    else:
        print("Geçersiz komut.")
    print("Güncel Değer:", sayac)
print("-" * 20)


