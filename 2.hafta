# 25. Asal Çarpanlarını Bulma
print("--- 25. Asal Çarpanları Bulma ---")
asal_sayi = int(input("Asal çarpanları bulunacak sayıyı girin: "))
bolen = 2
carpanlar = []
temp_sayi = asal_sayi
while bolen * bolen <= temp_sayi:
    if temp_sayi % bolen:
        bolen += 1
    else:
        temp_sayi //= bolen
        carpanlar.append(bolen)
if temp_sayi > 1:
    carpanlar.append(temp_sayi)
print(f"{asal_sayi} sayısının asal çarpanları:", carpanlar)
print("-" * 20)


