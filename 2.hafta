class Insan:
    def __init__(self, ad, yas):
        self.ad = ad
        self.yas = yas
        print(f"{self.ad} adında bir insan nesnesi oluşturuldu.")

    def konus(self):
        print(f"{self.ad}: 'Merhaba, nasılsınız?' diyor.")


class Hoca(Insan):
    def __init__(self, ad, yas, sicil_numarasi):
        super().__init__(ad, yas)
        self.sicil_numarasi = sicil_numarasi
        print(f"Sicil No: {self.sicil_numarasi} olan bir hoca nesnesi oluşturuldu.")

    def konus(self):
        print(f"Hoca {self.ad}: 'Arkadaşlar, bugünkü dersimize hoş geldiniz.' diyor.")

    def ders_ver(self):
        print(f"Hoca {self.ad} ({self.sicil_numarasi}) şu anda ders veriyor.")


class Sekreter(Insan):
    def __init__(self, ad, yas, departman):
        super().__init__(ad, yas)
        self.departman = departman
        print(f"{self.departman} departmanında bir sekreter nesnesi oluşturuldu.")

    def konus(self):
        print(f"Sekreter {self.ad}: 'Buyurun, size nasıl yardımcı olabilirim?' diyor.")

    def randevu_ayarla(self):
        print(f"Sekreter {self.ad}, yeni bir randevu ayarlıyor.")


class Ogrenci(Insan):
    def __init__(self, ad, yas, ogrenci_numarasi):
        super().__init__(ad, yas)
        self.ogrenci_numarasi = ogrenci_numarasi
        print(f"Numarası {self.ogrenci_numarasi} olan bir öğrenci nesnesi oluşturuldu.")

    def konus(self):
        print(f"Öğrenci {self.ad}: 'Ders ne zaman başlıyor?' diye soruyor.")
    
    def ders_calis(self):
        print(f"Öğrenci {self.ad} ({self.ogrenci_numarasi}) şu anda ders çalışıyor.")



